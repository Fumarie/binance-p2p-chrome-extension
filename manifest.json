{
  "manifest_version": 2,
  "name": "binanceExt2",
  "version": "1.0",
  "icons": {
    "48": "icons/border-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.p2p.binance.com/ru/fiatOrder"],
      "js": ["binanceExt.js"]
    }
  ],
  "background": {
    "scripts": ["popup.js"]
  },
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_title": "BinExt",
    "default_popup": "popup.html"
  }
}